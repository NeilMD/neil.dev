#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/neilcydriccapistrano/Desktop/SAIT/Fourth Sem/DSGN-308/porfolio-site/node_modules/.pnpm/@tailwindcss+cli@4.0.8/node_modules/@tailwindcss/cli/dist/node_modules:/Users/neilcydriccapistrano/Desktop/SAIT/Fourth Sem/DSGN-308/porfolio-site/node_modules/.pnpm/@tailwindcss+cli@4.0.8/node_modules/@tailwindcss/cli/node_modules:/Users/neilcydriccapistrano/Desktop/SAIT/Fourth Sem/DSGN-308/porfolio-site/node_modules/.pnpm/@tailwindcss+cli@4.0.8/node_modules/@tailwindcss/node_modules:/Users/neilcydriccapistrano/Desktop/SAIT/Fourth Sem/DSGN-308/porfolio-site/node_modules/.pnpm/@tailwindcss+cli@4.0.8/node_modules:/Users/neilcydriccapistrano/Desktop/SAIT/Fourth Sem/DSGN-308/porfolio-site/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/neilcydriccapistrano/Desktop/SAIT/Fourth Sem/DSGN-308/porfolio-site/node_modules/.pnpm/@tailwindcss+cli@4.0.8/node_modules/@tailwindcss/cli/dist/node_modules:/Users/neilcydriccapistrano/Desktop/SAIT/Fourth Sem/DSGN-308/porfolio-site/node_modules/.pnpm/@tailwindcss+cli@4.0.8/node_modules/@tailwindcss/cli/node_modules:/Users/neilcydriccapistrano/Desktop/SAIT/Fourth Sem/DSGN-308/porfolio-site/node_modules/.pnpm/@tailwindcss+cli@4.0.8/node_modules/@tailwindcss/node_modules:/Users/neilcydriccapistrano/Desktop/SAIT/Fourth Sem/DSGN-308/porfolio-site/node_modules/.pnpm/@tailwindcss+cli@4.0.8/node_modules:/Users/neilcydriccapistrano/Desktop/SAIT/Fourth Sem/DSGN-308/porfolio-site/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@tailwindcss+cli@4.0.8/node_modules/@tailwindcss/cli/dist/index.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/@tailwindcss+cli@4.0.8/node_modules/@tailwindcss/cli/dist/index.mjs" "$@"
fi
